{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "projectId": "af525c89-a5e4-45dd-a6f4-e16245d56bb8",
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "react-native-video",
      "expo-audio",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.raakanksha.boltexponativewind"
    },
    "extra": {
      "BASE_URL": "https://api2.teleplay.in",
      "router": {},
      "eas": {
        "projectId": "af525c89-a5e4-45dd-a6f4-e16245d56bb8"
      }
    },
    "owner": "raakanksha"
  }
}
